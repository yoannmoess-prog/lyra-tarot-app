{
  "name": "lyra-tarot-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run rag:rebuild && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite --host",
    "dev:server": "node server/index.js",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "npm run lint && npm run build",
    "smoke": "bash scripts/smoke.sh",
    "build:rag": "node scripts/build-rag.js",
    "rag:build": "node scripts/md2jsonl.mjs fiches build/rag/index.jsonl",
    "rag:check": "node scripts/md2jsonl.mjs fiches build/rag/index.jsonl --dry",
    "rag:embed": "node scripts/embed.mjs build/rag/index.jsonl build/rag/index.vec.jsonl",
    "rag:rebuild": "npm run rag:build && npm run rag:embed"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "express-rate-limit": "^8.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.4.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "gray-matter": "^4.0.3",
    "remove-markdown": "^0.6.2",
    "vite": "^7.1.2"
  }
}
